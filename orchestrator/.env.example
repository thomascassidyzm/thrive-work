# THRIVE Web Orchestrator Configuration

# Server Port
PORT=4568

# API Provider: 'claude' or 'openai'
API_PROVIDER=claude

# Claude API Key (get from https://console.anthropic.com/)
CLAUDE_API_KEY=your_claude_api_key_here

# OpenAI API Key (optional, get from https://platform.openai.com/)
OPENAI_API_KEY=your_openai_api_key_here

# Production URL (for CORS)
WEB_APP_URL=https://thrive-website.vercel.app

# Node Environment
NODE_ENV=production