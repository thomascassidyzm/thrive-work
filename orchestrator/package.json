{
  "name": "thrive-web-orchestrator",
  "version": "1.0.0",
  "description": "THRIVE Web Orchestrator - HTTP server for coaching requests",
  "type": "module",
  "main": "thrive-local-orchestrator.js",
  "scripts": {
    "start": "node thrive-local-orchestrator.js",
    "dev": "node thrive-local-orchestrator.js",
    "pm2:start": "pm2 start ecosystem.config.js",
